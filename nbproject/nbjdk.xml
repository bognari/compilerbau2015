<?xml version="1.0" encoding="UTF-8"?>
<project basedir=".." name="MiniJava">
    <property file="nbproject/nbjdk.properties"/>
    <property location="${netbeans.user}/build.properties" name="user.properties.file"/>
    <property file="${user.properties.file}"/>
    <import file="jdk.xml"/>
    <target depends="-jdk-init" name="compile">
        <ant inheritall="false" target="compile"/>
    </target>
    <target depends="-jdk-init" name="clean">
        <ant inheritall="false" target="clean"/>
    </target>
    <target depends="-jdk-init" name="simpletest">
        <ant inheritall="false" target="simpletest"/>
    </target>
    <target depends="-jdk-init" name="junit">
        <ant inheritall="false" target="junit"/>
    </target>
</project>
